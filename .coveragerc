[run]
source = app
omit = 
    */tests/*
    */venv/*
    */node_modules/*
    */__pycache__/*
    */migrations/*
    */.pytest_cache/*
    setup.py
    run_*.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = coverage/html
show_contexts = true
skip_covered = false

[json]
output = coverage/coverage.json
pretty_print = true

[xml]
output = coverage/coverage.xml